                                        </tr>
                                        <tr>
                                            <td>Perception</td>
                                            <td><%= displayCharacter.perception_skill %></td>
                                            <td>0</td>
                                            <td><%= displayCharacter.perception_skill %></td>
                                        </tr>
                                        <tr>
                                            <td>Larceny</td>
                                            <td><%= displayCharacter.larceny_skill %></td>
