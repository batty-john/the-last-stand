                                                    <label for="hair">Race</label>
                                                    <input placeholder="Black" type="text" name="hair" id="hair" class="form-control" value="<%=displayCharacter.race%>" readonly/> 
                                                </div>
                                                <div class="col">
                                                    <label for="eyes">Class</label>
                                                    <input placeholder="Blue" type="text" name="Eyes" id="eyes" class="form-control" value="<%=displayCharacter.class%>" readonly/> 
                                                </div>
                                                <div class="col">
                                                    <label for="skin">Major Background</label>
                                                    <input placeholder="Pale" type="text" name="skin" id="skin" class="form-control" value="<%=displayCharacter.major_background%>" readonly/> 
                                                </div>
