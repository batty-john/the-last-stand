                                        </tr>
                                        <tr>
                                            <td>Survival</td>
                                            <td><%= displayCharacter.survival_skill %></td>
                                            <td>0</td>
                                            <td><%= displayCharacter.survival_skill %></td>
                                        </tr>
                                      </tbody>
                                    </table>   
